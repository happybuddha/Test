/*SQL update script for release v1.35 */

/* changing the name of the */
select @causeId := (select cause_id from cause where name = "Treetops for Titis");
update cause set name = "Treetops for Tit&#237;s" where cause_id = @causeId;


/* #1159 - point 3b */
select @causeId := (select cause_id from cause where name = "Treetops for Tit&#237;s");
update cause set parent_summary_text='FiftyP members are preserving the habitat of the endangered tit&#237; monkey, or cotton top tamarin. With much of their habitat cleared for ranching, the tit&#237;s live in isolated patches of forest. Linking these areas with new forest growth will give the tit&#237;s a bigger home and a better chance of survival.\r\n\r\nFiftyP: Helping kids learn to do well by doing good' where cause_id = @causeId;
select @causeId := (select cause_id from cause where name = "Share the Music");
update cause set parent_summary_text='FiftyP members are putting musical instruments into the hands of students in underserved schools and community music programs. The children served by this project are able to nurture their love of music and develop a means of expression that will stay with them for life.\r\n\r\nFiftyP: Helping kids learn to do well by doing good' where cause_id = @causeId;
select @causeId := (select cause_id from cause where name = "Safe Stomping Grounds");
update cause set parent_summary_text='FiftyP members are building a sanctuary for rescued bull elephants. With spacious barns, acres of enclosed habitat, and cool lakes to splash in, these mammoth majestic animals can enjoy a life of dignity and peace.\r\n\r\nFiftyP: Helping kids learn to do well by doing good'where cause_id = @causeId ;
select @causeId := (select cause_id from cause where name = "Prevent Malaria");
update cause set parent_summary_text='FiftyP members are protecting children and pregnant women from the mosquitoes that transmit malaria. An inexpensive bed net can prevent this life-threatening disease by providing an effective barrier overnight, when mosquitoes are most active. \r\n\r\nFiftyP: Helping kids learn to do well by doing good' where cause_id = @causeId;

/* #1159 - point 7*/
select @causeId := (select cause_id from cause where name = "Treetops for Tit&#237;s");
update share_store_item set description= "There's no coffee shop in the forest, so workers bring their own.", thank_you_msg = "Thanks for supporting the Treetops for Titis project. There's no coffee shop in the forest, so workers bring their own."  where name = 'Thermos' and cause_id = @causeId;

select @causeId := (select cause_id from cause where name = "Share the Music");
update share_store_item set description= "What's better than a pizza break during a long rehearsal?", thank_you_msg = "Thanks for supporting the Share the Music program. What's better than a pizza break during a long rehearsal?"  where name = 'Pizza' and cause_id = @causeId;

select @causeId := (select cause_id from cause where name = "Safe Stomping Grounds");
update share_store_item set description= "Elephants love cookies, even when there's medicine inside.", thank_you_msg = "Thanks for supporting the Safe Stomping Grounds project. Elephants love cookies, even when there's medicine inside."  where name = 'Cookies' and cause_id = @causeId;
update share_store_item set description= " A long handle makes it easier to scrub an elephant's back!", thank_you_msg = "Thanks for supporting the Safe Stomping Grounds project. A long handle makes it easier to scrub an elephant's back!"  where name = 'Scrub brush' and cause_id = @causeId;

/* updating   6th point */
select @causeId := (select cause_id from cause where name = "Treetops for Tit&#237;s");
update cause set description="The tiny tit&#237; monkey - no bigger than a squirrel, with huge eyes, a black face, and a shock of white hair - is at risk of losing its home. Also known as cotton top tamarins, these endangered monkeys live in one small part of the world - the forests of Colombia, South America. But the forests are fast disappearing, cut down by local villagers for farming and raising cattle.<br /><br />The Tit&#237;s are forced to live in small. The Treetops for Tit&#237;s project will link these separate areas by planting \"live fences\": trees and vines that fill in the gaps. With your help, tit&#237;s will gain a bigger and better habitat - and a greater chance that the species will survive.", hos_summary_txt = "to Treetops for tit&#237;s" where cause_id = @causeId ;

select @causeId := (select cause_id from cause where name = "Share the Music");
update cause set description="Who doesn''t love music? Even babies can get with the rhythm by clapping, wiggling, and banging along on pots. But many kids who want to play a real instrument never get the chance. Their schools may not have a music program, or enough instruments for all who want to play.\r\nYou can help by supporting a program that donates musical instruments to schools, community music programs, and individual students in need. These instruments have made it possible for more than a half-million kids to pursue their love of music."  where cause_id = @causeId ;

select @causeId := (select cause_id from cause where name = "Safe Stomping Grounds");
update cause set description="What happens to performing elephants when they''re no longer needed? Too often it''s a sad story, especially for bull elephants. Because these mammoth males are so large and strong, providing a safe habitat is expensive. Instead of enjoying a comfortable home, many bulls are kept chained in small spaces.\r\nNicholas is a former circus elephant that has been luckier than most. He was rescued and brought to a wildlife sanctuary, where he lives in a large barn on a grassy hillside. The Safe Stomping Grounds project hopes to rescue other bull elephants - as soon as it builds more big barns and strong fences."  where cause_id = @causeId ;

select @causeId := (select cause_id from cause where name = "Prevent Malaria");
update cause set description="In some countries the mosquitoes are more than just pests. They carry malaria, a serious and sometimes deadly disease. Malaria hits hardest in children under age 5, and millions are affected each year.\r\nThere''s a simple, inexpensive way to protect kids from malaria: Because mosquitoes are most likely to bite at night, using a bed net dipped in insecticide can keep a family safe. \r\nWith your help, lifesaving bed nets can be provided to families in developing countries where malaria is widespread. Health workers deliver the nets to rural villages and teach families how to properly use them. They also spray insecticide on the walls inside each home - another tool in the fight against malaria."  where cause_id = @causeId ;


/* #1158 */
ALTER TABLE cause ADD share_store_text TEXT NULL ;

select @causeId := (select cause_id from cause where name = "Treetops for Tit&#237;s");
UPDATE cause SET share_store_text = 'Preserve the habitat of endangered monkeys whose forest home is being cut down! Linking separate patches of forest will give cotton-top tamarins (also called tit&#237;s) a bigger home and a better chance to survive.' WHERE cause_id = @causeId;

select @causeId := (select cause_id from cause where name = "Share the Music");
UPDATE cause SET share_store_text = 'Give kids who love music the chance to play! Supplying musical instruments to schools that can''t buy them lets everyone get in the act.' WHERE cause_id = @causeId;

select @causeId := (select cause_id from cause where name = "Safe Stomping Grounds");
UPDATE cause SET share_store_text = 'Help build a safe home for performing bull elephants that have been rescued from harm. A large barn and acres of land will let them live in dignity and peace.' WHERE cause_id = @causeId;

select @causeId := (select cause_id from cause where name = "Prevent Malaria");
UPDATE cause SET share_store_text = 'Protect kids from mosquitoes that carry malaria, a serious and often life-threatening disease. A simple bed net can keep an entire family safe overnight, when mosquitoes are most likely to bite.' WHERE cause_id = @causeId;


/*
For the all comments of point 8 from #1159
*/
select @causeId:=(select cause_id from cause where name = "Treetops for Tit&#237;s" and month="6");
UPDATE cause
SET hos_full_txt ='Saves ^Forest for Rare Monkeys!# The tropical forests of Colombia, South America, @have been<br /> shrinking - bad news for the tiny tit&#237; monkey, also known as <br />cotton top tamarins. <br /><br />But a solution is at hand, thanks to <b>ChildName</b> of FiftyP.
<br /><br />These forests are the monkeys'' only home, but large sections<br /> have been cleared to use for farming and raising cattle. Now the tit&#237;s are crowded into small, separate patches of forest - a situation that can lead to conflict and disease.
<br /><br />By donating dollaramount from gender Share Account, <b>ChildName</b> has saved the day for these charming, endangered monkeys. The money is used to plant trees and vines to fill in the gaps between sections of forest. Soon the titis will once again be able to leap from tree to tree - in a bigger and healthier habitat.'
WHERE cause_id =@causeId;

select @causeId:=(select cause_id from cause where name = "Safe Stomping Grounds" and month="6");
UPDATE cause
SET hos_full_txt ='Rescues ^Bull Elephants!# Bull elephants are trumpeting their thanks to <b>ChildName</b> of <br/>FiftyP, @who reached out to help these majestic creatures with <br/>a dollaramount donation from gender Share Account. The money will <br/>go toward building a safe, spacious habitat for circus <br/>elephants that no longer perform. Housing and feeding a <br/>10,000-pound bull elephant is hugely expensive, and many are <br/>neglected - or worse - once they''ve circled the ring for the <br />last time.  
<br/><br/>The Safe Stomping Grounds project has already rescued Nicholas, a bull that rode a tricycle in the circus at age two. Now age 15, he spends his days grazing on the hillside and splashing in the lake of his large, peaceful habitat. Soon other bull elephants will get the homes they deserve, thanks to <b>ChildName</b> and others who care.'
WHERE cause_id =@causeId;

select @causeId:=(select cause_id from cause where name = "Share the Music" and month="6");
UPDATE cause 
SET hos_full_txt ='Keeps^ the Music Going!# School bands and orchestras are a source of pride and school <br />spirit - and @ a way for kids who love music to develop their <br />talent. So it can be disappointing if a school doesn''t have an <br />instrument for every student, or the funds to fix equipment <br />that gets broken.
<br /><br />Luckily there are people like <b>ChildName</b>, who is supporting a <br /> program that donates musical instruments to underserved schools, community music programs, and individual students in need. <b>ChildName</b> contributed dollaramount from gender FiftyP Share Account to give young musicians the chance to learn, practice, and take a well-deserved bow.
<br/><br />Here''s a round of applause for <b>ChildName</b>!'
WHERE cause_id =@causeId;

select @causeId:=(select cause_id from cause where name = "Prevent Malaria" and month="6");
UPDATE cause
SET hos_full_txt ='Outsmarts^ Mosquitoes!# In some of the poorest parts of the world, mosquitoes are a <br/>major health threat. @Their bite leaves more than just an itch.<br/> It can transmit malaria, a serious and sometimes deadly disease.
<br/><br/>That''s why <b>ChildName</b> has joined the effort to prevent these<br/>
 bugs from biting. personalPronoun donated dollaramount from gender <br/>Share Account to provide lifesaving bed nets to developing <br />countries where malaria is widespread. Mosquitoes mostly bite at night, so using a bed net treated with insecticide can keep kids - and their whole family - safe.<br/><br/> Thank you, <b>ChildName</b>, for protecting kids against mosquitoes and malaria!	'
WHERE cause_id =@causeId;

