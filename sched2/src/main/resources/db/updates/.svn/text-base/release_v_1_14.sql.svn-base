INSERT INTO sys_template (sys_template_id, description, design, subject) VALUES
('CANCELLATION_ACCOUNT_NOTIFICATION', 'Email template for account cancellation notification','<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN""http://www.w3.org/TR/html4/loose.dtd">\r\n<html>\r\n<head>\r\n<title>FiftyP</title>\r\n<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">\r\n</head>\r\n<body>\r\n<table width="600" border="0" align="center" cellpadding="0" cellspacing="0" style="border:1px solid #7B2B83">\r\n  <tr>\r\n    <td style="border-bottom:1px solid #7B2B83">\r\n      <table width="100%" border="0">\r\n        <tr>\r\n          <td><img src="cid:logo_parent.gif" width="294" height="84"></td>\r\n          <td align="right" style="padding:55px 10px 0px 0px" ><font color="#F68B1F" size="2" style="font-size:14px;" face="verdana"><b>\r\n			FiftyP account cancellation notification!\r\n		  </b></font></td>\r\n        </tr>\r\n    </table></td>\r\n  </tr>\r\n  <tr>\r\n    <td style="padding:20px;font-size:12px; font-family:verdana;">\r\n	Dear FiftyP Parent,<br/><br/>  We received your request to cancel your family''s FiftyP account as of ${cancellationDate?string("MMMM d, yyyy")}. We''re sorry to see you go! We trust that your experience with FiftyP has helped put your family on the path to financial fitness.  \r\n	<br/><br/>	Beginning today you will no longer be able to log in to FiftyP or to access your account information. Please note that because half our profits are donated to charitable causes there can be no refunds.<br/><br/> Should you wish to reinstate your membership, please contact us by ${reinstateDate?string("MMMM d, yyyy")}  (ie, within 30 days) at <a href=''http://www.fiftyp.com''>support@fiftyp.com</a>. After that date your account information will be permanently deleted. <br/><br/>\r\n	In times like these it''s hard to minimize the value of teaching our kids to be financially and socially responsible. We hope you''ll reconsider FiftyP if you find you need support as you work toward these goals.\r\n	<br/><br/>You''re always welcome back to the FiftyP family!  <br/><br/>\r\n	With best wishes,<br>\r\n			The FiftyP Team<br>\r\n			<a href=''http://www.fiftyp.com''>www.fiftyp.com</a><br>\r\n	  </p>\r\n	</td>\r\n  </tr>\r\n  <tr>\r\n    <td bgcolor="#7B2B83" style="padding:5px"> <font  style="font-size:11px; line-height:16px" face="verdana" color="#ffffff">For more information please contact our support team at </font><a href=''http://www.fiftyp.com''><font  style="font-size:11px; line-height:16px" face="verdana" color="#ffffff">support@fiftyp.com</font></a><br>\r\n    </td>\r\n  </tr>\r\n</table>\r\n\r\n</body>', 'FiftyP account cancellation notification');
